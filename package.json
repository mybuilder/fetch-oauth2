{
  "name": "fetch-oauth2",
  "version": "0.1.5",
  "description": "OAuth2 authorization library for fetch requests",
  "repository": "https://github.com/mybuilder/fetch-oauth2",
  "main": "index.js",
  "scripts": {
    "start": "echo '{\"port\": 3901}' > \"$PWD/test_server_port.json\" && node ./script/server 3901",
    "test": "./script/test",
    "build": "babel src --out-dir lib",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "publish-patch": "npm version patch && npm publish",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "fetch",
    "fetchjs",
    "request",
    "http",
    "auth",
    "authorization"
  ],
  "author": "Sten",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.0.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.0.0",
    "babel-runtime": "^5.8.29",
    "expect.js": "^0.3.1",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.3",
    "karma": "^2.0.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^2.0.10",
    "mocha": "^4.0.0",
    "phantomjs": "^2.1.1",
    "wallaby-webpack": "3.9.8",
    "webpack": "^2.2.0",
    "whatwg-fetch": "^0.10.1"
  },
  "snyk": true,
  "dependencies": {
    "snyk": "^1.89.1"
  }
}
